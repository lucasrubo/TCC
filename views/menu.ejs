<div class="hero_area">
  <!-- header section strats -->
  <header class="header_section">
    <div class="container-fluid">
      <nav class="navbar navbar-expand-lg custom_nav-container ">
        <a class="navbar-brand" href="/">
          <span>
            <img class="logo-png" src="../images/patinha.png">
          </span>
        </a>      
        <button class="navbar-toggler ml-auto" type="button" data-toggle="collapse"
          data-target="#navbarSupportedContent" aria-controls="navbarSupportedContent" aria-expanded="false"
          aria-label="Toggle navigation">
          <span class="navbar-toggler-icon"></span>
        </button>
        <div class="collapse navbar-collapse" id="navbarSupportedContent">
          <div class="d-flex mx-auto flex-column flex-lg-row align-items-center">
            <ul class="navbar-nav  ">
              <% if(userValues.type != "normal" && userValues.type){ %>
              <li id="inicio-menu" class="nav-item dropdown">
                <a class="nav-link link-direto <% if(variant == 'index'){ %><%= "active" %><% } %>" <% if(variant == 'index'){ %><%= "href=#inicio" %><% } else { %><%= "href=../" %><% } %>>Início</a>                
                  <div class="dropdown-content">
                    <a class="link-direto" <% if(variant == 'index'){ %><%= "href=#quem-somos" %><% } else { %><%= "href=../#quem-somos" %><% } %>> Sobre</a>
                    <a class="link-direto" <% if(variant == 'index'){ %><%= "href=#servicos" %><% } else { %><%= "href=../#servicos" %><% } %>> Serviços</a>
                    <a class="link-direto" <% if(variant == 'index'){ %><%= "href=#contato" %><% } else { %><%= "href=../#contato" %><% } %>> Contato</a>
                  </div>
              </li>
              <li id="sistemas" class="nav-item dropdown">
                <a class="nav-link link-direto <% if(variant == 'sistema'){ %><%= "active" %><% } %>" href="../sistema">Sistemas</a>              
                <div class="dropdown-content">
                  <a href="/sistema/usuarios" class="link-direto" >Úsuarios</a>
                  <a href="/sistema/vacinas" class="link-direto" >Vacinas</a>
                  <a href="/sistema/animais" class="link-direto" >Animais</a>
                  <a href="/sistema/cadastro-animais" class="link-direto" >Cadastro Animais</a>
                </div>
              </li>
              <% }else{  %>
                <li id="inicio-menu" class="nav-item">
                  <a class="nav-link link-direto" <% if(variant == 'index'){ %><%= "href=#inicio" %><% } else { %><%= "href=../" %><% } %>>Início</a> 
                </li> 
                <li id="inicio-menu" class="nav-item">
                  <a class="nav-link link-direto" <% if(variant == 'index'){ %><%= "href=#quem-somos" %><% } else { %><%= "href=../#quem-somos" %><% } %>> Sobre</a>
                </li>
                <li id="inicio-menu" class="nav-item">
                  <a class="nav-link link-direto" <% if(variant == 'index'){ %><%= "href=#servicos" %><% } else { %><%= "href=../#servicos" %><% } %>> Serviços</a>
                </li>
                <li id="inicio-menu" class="nav-item">
                  <a class="nav-link link-direto" <% if(variant == 'index'){ %><%= "href=#contato" %><% } else { %><%= "href=../#contato" %><% } %>> Contato</a>
                </li>
                <% } %>
                <li id="inicio-menu" class="nav-item">
                  <a class="nav-link link-direto <% if(variant == 'mapa'){ %><%= "active" %><% } %>" href="../mapa">Mapa</a> 
                </li> 
            </ul>
          </div>
        </div>
          
        <div class="dropdown dropdown-user">
          <span class="usuario"><%= userValues.username %></span>
          <a class="navbar-user">
            <span>
              <% if(userValues.imagem){ %>
              <img class="user-logo" src="../upload/<%= userValues.imagem %>" onerror="this.src='../images/usuario-branco.png'">
              <% }else{ %>      
              <img src="../images/usuario-branco.png" alt="" class="user-logo" onerror="this.src='../images/usuario-branco.png'" />  
              <% } %>            
            </span>
          </a>                       
          <div class="dropdown-content" style="right:0;margin-top: 4px;">
            <% if(userValues.username){ %>
            <a href="../usuario/perfil" class="link-direto">Configurações</a>   
            <form method="GET" action="../logout">
              <button type="submit" class="link-direto botao-sair">Sair</button>
            </form>         
            <% }else{ %>      
              <a href="#" id="login" class="nav-link" >Login</a>  
              <a href="/cadastro-usuario-normal" id="Cadastrar" class="nav-link" >Cadastrar</a>     
            <% } %>              
          </div>
        </div>
      </nav>
    </div>
  </header>
  <!-- end header section -->
</div>